/* pages/list/list.wxss */
.container{
	flex: 1;
	background: #fff;
	display: flex;
	flex-direction: column;
}
.header{
	font-size: 14px;
	color: #666;
	height: 260rpx;
	padding-top: 30rpx;
	position: relative;
}
.list{
	flex:1;
	display: flex;
	overflow: auto;
	-webkit-overflow-scrolling: touch;
}

.icon-search{
	width: 24rpx;
	height: 24rpx;
	margin-right: 13rpx;
	vertical-align: middle;
}
.searchbox{
	background: rgba(255, 255, 255, .9);
	border-radius: 50rpx;
	height: 60rpx;
	width: 400rpx;
	padding: 10rpx 24rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
	display: flex;
	align-items: center;
	font-size: 14px;
}
.input-search{
	text-indent: 10rpx;
}
.icon-shop{
	width: 62rpx;
	height: 62rpx;
}
.row{
	display: flex;
}
.row-center{
	display: flex;
	justify-content: center;
	align-items: center;
}
.btn-search{
	padding: 10rpx 22rpx;
	font-size: 14px;
	color: #fff;
	background: #ff9267;
	border-radius: 50rpx;
}
.footer{
	display: flex;
	height: 110rpx;
	
}
.btn-settle{
	background: #ff9267;
	color: #fff;
	font-size: 18px;
	padding: 30rpx 56rpx;
}
.footer-left{
	flex:1;
	 background: #333; 
	 position: relative;
	 padding: 24rpx 0;
}

.shop-cart{
	position: absolute;
	 background: #333;  
	width: 134rpx;
	height: 134rpx;
	top: -36rpx;
	left: 50rpx;
	border-radius: 50%;
	padding: 16rpx;
}
.mask .shop-cart{
	top: -10rpx;
	left: 24rpx;
}
.cart{
	width: 105rpx;
	height: 105rpx;
	background:url(data:image/png;base64,) center  no-repeat #000;
	background-size: 56rpx 52rpx;
	border-radius: 50%;
	
}
.mask .cart{
	width: 105rpx;
	height: 105rpx;
	background: url(data:image/png;base64,) center  no-repeat #ff9267;
	background-size: 56rpx 52rpx;
	border-radius: 50%;
}
.number{
	position: absolute;
	height: 40rpx;
	color: #fff;
	border-radius: 20rpx;
	padding: 0 13rpx;
	font-size: 12px;
	line-height: 40rpx;
	background: #ff9267;
	top: 0;
	right: 0rpx;
}
.total-price{
	color: #fff;
	font-size: 14px;
	margin-left: 200rpx;
}

.total-price text{
	font-size: 18px;
}

.type-list{
	width: 150rpx;
	background: #efeff4;
	font-size: 12px;
	color: #333;
	overflow: auto;
	-webkit-overflow-scrolling: touch;
}
.type{
	height: 130rpx;
	vertical-align: middle;
	text-align: center;
	display: flex;
	align-items: center;
	justify-content: center;
	border-bottom: 1rpx solid #ddd;


}
.type.active{
	border-left: 6rpx solid #ff9267;
	background: #fff;
	border-bottom: 0;
}
.all-goods{
overflow: auto;
	flex: 1;
	-webkit-overflow-scrolling: touch;
}
.goods{
	padding-left: 10rpx;
	
}
.icon-btn{
	width: 50rpx;
	height: 50rpx;
}
.good{
	padding: 30rpx 30rpx 30rpx 20rpx;
}
.good+.good{
	border-top: 1rpx solid #ddd;
}
.good-photo{
	width: 110rpx;
	height: 110rpx;
	margin-right: 20rpx;
}

.good-name{
	font-size: 16px;
	color: #000;
	font-weight: bold;
	margin-bottom: 15rpx;
}
.price{
	color: #ff5c05;
	font-weight: bold;
}
.price label{
	font-size: 12px;
}
.price text{
	font-size: 14px;
}
.between{
	display: flex;
	justify-content: space-between;
}
.good-info{
	flex:1;
}
.operation{
	display: flex;
	align-items: center;
}
.operation text{
	margin: 0 30rpx;
	width: 30rpx;
	text-align: center;
}

.type-name{
	background: #efeff4;
	font-size: 12px;
	padding: 10rpx;
	height:60rpx;
}

/*****购物车弹框*****/
.mask{
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background: rgba(0, 0, 0, .5);
	z-index: 9;
}

.mask-content{
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	padding-top: 140rpx;
}
.mask .shop-cart{
	background: transparent;
}
.bags{
	background: #fff;
	height: 20rem;
}
.bag-goods{
	min-height: 150rpx;
}
.bag-good{
	display: flex;
	justify-content: space-between;
	align-items: center; 
	height: 150rpx;
	padding: 0 20rpx;
}
.bag-good+.bag-good{
	border-top: 1rpx solid #ddd;
}
.bag-good .price{
	flex: 1;
	text-align: right;
	margin-right: 56rpx;
}
.bag-good .good-name{
	margin-bottom: 10rpx;
}
.guige{
	font-size: 12px;
	color: #999;
}

.bag-header{
	background: #efeff4;
	border-left: 4rpx solid #ff9267;
	padding-left: 16rpx;
	padding-right: 24rpx;
	height: 70rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.btn-delete{
	width:32rpx;
	height: 36rpx; 	
}
.mask .total-price{
	margin-left: 24rpx;
}

.moving{
	position: absolute;
	opacity: .7;
	z-index: 10;
	width: 100%;
}
.none{
		position: absolute;
	opacity: 0;
	z-index: 10;
	width: 100%;
}
.scroll{
	overflow-y: auto;
}
.disable-scroll{
	overflow-y: hidden;
}
/*mask**/

.icon-close{
	width: 20rpx;
	height: 20rpx;
}

.format-panel{
	position: absolute;
	width: 640rpx;
	border-radius: 4rpx;
	background: #fff;
	left: 50%;
	top: 50%;
	transform: translate(-50%,-50%);
}

.panel-header{
	padding: 28rpx 30rpx 0;
	display: flex;
}
.panel-title{
	font-size: 16px;
	font-weight: bold;
	color: #000;
	flex: 1;
	margin-right: 70rpx;
	word-break: break-all;
}

.panel-body{
	padding: 0	10rpx 56rpx 30rpx; 
}
.option{
	margin-top: 40rpx;
	font-size: 14px;
	color:#999;
}
.labels{
	display: flex;
	padding-left: 5rpx;
	margin-top: 15rpx;
	flex-wrap: wrap;
	
}
.label{
	border: 1rpx solid #999;
	border-radius: 50rpx;
	padding: 10rpx 30rpx;
	margin-right: 20rpx;
	margin-top: 10rpx;
}
.label.active{
	background: #ff9267;
	color: #fff;
	border-color: #fff;
}
.panel-footer{
	background: #f5f5fb;
	padding: 0 30rpx;
	height: 110rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.panel-footer-price{
	color: #ff5c05;
	font-size: 14px;
	font-weight: bold;
}
.panel-footer-price label{
	font-size: 12px;
}
.panel-footer-btn{
	font-size: 16px;
	padding: 8rpx 27rpx;
	color: #fff;
	background: #ff9267;
	border-radius: 2rpx;
}
.btn-selectFormat{
	background: #ff9267;
	padding: 5rpx 18rpx;
	color: #fff;
	border-radius: 80rpx;
	font-size: 14px;
}

.swiper{
	width: 100%;
	height: 56rpx;
	background:rgba(0,0,0,0.60);
	position: absolute;
	left: 0;
	bottom: 0;
	color: #fff;
	font-size: 12px;
}
.swiper swiper-item{
	padding: 12rpx 20rpx;
}
.no-data{
	text-align: center;
	padding-top: 265rpx;
	font-size: 14px;
	color: #999;
}
.icon-no-data{
	width: 120rpx;
	height: 120rpx;
	margin-bottom: 30rpx;
}

